<p id="notice"><%= notice %></p>

<h1>Listing Menus</h1>

<table>
  <thead>
    <tr>
      <th>Position</th>
      <th>Nombre</th>
      <th>Visible</th>
      <th>Background</th>
      <th>Label color</th>
      <th>Width</th>
      <th>Height</th>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @menus.each do |menu| %>
      <tr>
        <td><%= menu.position %></td>
        <td><%= menu.nombre %></td>
        <td><%= menu.visible %></td>
        <td><%= menu.background %></td>
        <td><%= menu.label_color %></td>
        <td><%= menu.width %></td>
        <td><%= menu.height %></td>
        <td><%= menu.url %></td>
        <td><%= link_to 'Show', menu %></td>
        <td><%= link_to 'Edit', edit_menu_path(menu) %></td>
        <td><%= link_to 'Destroy', menu, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Menu', new_menu_path %>
